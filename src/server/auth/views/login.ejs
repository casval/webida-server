<body>

<div id="wrapper" class="container-login">
    <header class="header-logo">
        <h1>webida</h1>
    </header>

    <article>
        <section class="container-section container_login">
            <div class="login-box">
                <form role="form" onsubmit="return false;">
                    <div class="login_failed hide">
                        <div class="login_label">Log-in failed</div>
                        <div class="login_failed_msg">Invalid user name or password</div>
                    </div>
                    <fieldset>
                        <div class="form-group">
                            <input class="form-text data-email" placeholder="E-mail" name="email" type="email" autofocus>
                        </div>
                        <div class="form-group">
                            <input class="form-text data-password" placeholder="Password" name="password" type="password" value="">
                        </div>
                        <div class="form-group">
                            <button id="loginbutton" class="button util primary login action-login">Login</button>
                        </div>
                        <div class="form-group">
                            <div class="form-checkbox">
                                <input id="remember" class="data-remember" name="remember" type="checkbox" value="Remember Me">
                                <label for="remember">Remember my email</label>
                            </div>
                            <div class="form-password">
                                <a href="#" class="action-toggle-page" data-page="findPassword">Forgot your password?</a>
                            </div>
                        </div>
                        <div class="button_group clear">
                            <button class="button logtype primary google" onclick="location.href='/api/google';">Connect with Google</button>
                            <button class="button logtype primary github" onclick="location.href='/api/github';">Connect with GitHub</button>
                        </div>
                    </fieldset>
                </form>
            </div>
        </section>

        <section class="container-section container_passwd">
            <div class="login-box">
                <form role="form" onsubmit="return false;">
                    <fieldset>
                        <div class="login_failed hide">
                            <div class="login_label">Password reset failed</div>
                            <div class="login_failed_msg">Invalid user email</div>
                        </div>
                        <a class="action-toggle-page" data-page="login" href="#" tabindex="-1">Back</a>
                        <div class="form-group pwtitle">
                            Forgot your password?
                        </div>
                        <div class="form-group pwdesc">
                            Please enter your email address below and<br>we'll email you instructions how to reset it.
                        </div>
                        <div class="form-group">
                            <input class="form-text data-email" placeholder="Email" id="email_pw" tabindex="-1" type="text">
                        </div>
                        <div class="form-group">
                            <input class="button logbox primary login action-find-password" value="Submit" tabindex="-1" type="button">
                        </div>
                        <form role="form">
                            <div class="container_passwd_success hide">
                                <div class="passwd_success_label">
                                    We've sent an email with instructions on how to reset your password.
                                </div>
                                <div class="passwd_success_label2">
                                    If you don't receive instructions within a few minutes, check your email's spam and junk filters.
                                </div>
                            </div>
                        </form>
                </fieldset>
                </form>
            </div>
        </section>
    </article>

    <footer class="footer">
        <div class="container-box"></div>
    </footer>

</div>


<!-- jQuery -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>

